<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HelenSkin</title>
    <link rel="stylesheet" href="~/css\bootstrap.min.css">
    <link rel="stylesheet" href="~/css\plugin.css">
    <link rel="stylesheet" href="~/css\bundle.css">
    <link rel="stylesheet" href="~/css\style.css">
    <link rel="stylesheet" href="~/css\responsive.css">
    <link rel="stylesheet" href="~/css\css1.css">
    <script src="js\vendor\modernizr-2.8.3.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" integrity="sha384-F6XHp5x1OK9W5Dr9w5JgsfpfpF+Fg3b3z2XyXVgcrDfo8koHD8t07H+hw+BuLNFu" crossorigin="anonymous">
</head>
<body>

    <div class="pos_page">
        <div class="container">
            <!--pos page inner-->
            <div class="pos_page_inner">
                <!--header area -->
                <div class="pos_page_inner">
                    <!--header area -->
                    <div class="header_area">
                        <!--header top-->
                        <div class="header_top">
                            <div class="row align-items-center">
                                <div class="col-lg-6 col-md-6 mt-2">
                                    <a href="#"><h4 class="logo-helenskin">HelenSkin</h4></a>
                                </div>
                                <div class="col-lg-6 col-md-6 mt-2">
                                    <div class="header_links">
                                        <ul>
                                            <li><a href="ThongTinGiaoHang.html" title="Contact">TikTok</a></li>
                                            <li><a href="cart.html" title="My cart">Facebook</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--header top end-->
                        <div class="header_bottom">
                            <div class="row">
                                <div class="col-12">
                                    <div class="main_menu_inner">
                                        <div class="main_menu d-none d-lg-block">
                                            <nav class="flex-between">
                                                <ul>
                                                    <li class="active">
                                                        <a asp-area="" asp-controller="Home" asp-action="Index"> <i class="fas fa-light fa-store"></i> TRANNG CHỦ</a>

                                                    </li>
                                                    <li>
                                                        <a href="SanPham.html">SẢN PHẨM</a>

                                                    </li>
                                                    <li>
                                                        <a href="portfolio.html">GIỚI THIỆU</a>
                                                    </li>
                                                    <li class="user-icon">
                                                        <a href="#" data-toggle="dropdown">Quản lý</a>
                                                        <ul class="dropdown-menu">
                                                            <li><a class="pl-3" asp-area="" asp-controller="NguoiDung" asp-action="Index">Tài khoản</a></li>
                                                            <li><a class="pl-3" href="#">Danh mục</a></li>
                                                            <li><a class="pl-3" href="#">Sản phẩm</a></li>
                                                            <li><a class="pl-3" href="#">Đơn hàng</a></li>
                                                        </ul>
                                                    </li>
                                                </ul>
                                                <ul>
                                                    <li class="search-icon px-1">
                                                        <a href="#" id="searchToggle"><i class="fa fa-search"></i></a>
                                                        <div class="tk-trangchu" style="z-index: 100;">
                                                            <h6><strong>TÌM KIẾM</strong></h6>
                                                            <div class="search-box">
                                                                <input type="text" placeholder="Tìm kiếm sản phẩm...">
                                                                <button type="submit"><i class="fa fa-search"></i></button>
                                                            </div>
                                                        </div>
                                                    </li>

                                                    <li class="user-icon px-1 dropdown">
                                                        <a href="#" data-toggle="dropdown">
                                                            <i class="fa fa-user"></i>
                                                        </a>
                                                        <ul class="dropdown-menu">
                                                            <li><a class="pl-3" href="/login.html">Đăng nhập</a></li>
                                                            <li><a class="pl-3" href="/login.html">Thông tin tài khoản</a></li>
                                                        </ul>
                                                    </li>
                                                    <li class="px-1">
                                                        <a href="/GioHang.html"><i class="fa fa-shopping-cart"></i></a>
                                                    </li>
                                                </ul>
                                            </nav>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--header end -->
                <!-- customer login start -->
                <div>
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>

                <!-- customer login end -->

            </div>
            <!--pos page inner end-->
        </div>
    </div>
    <!--footer area start-->
    <div class="footer_area mt-3">
        <div class="footer_top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-6 col-sm-6">
                        <div class="footer_widget">
                            <h3>GIỚI THIỆU</h3>
                            <p>Công ty TNHH HOÀI THU TH</p>
                            <div class="footer_widget_contect">
                                <p>
                                    <i class="fa fa-map-marker" aria-hidden="true"></i> Địa chỉ trụ sở chính : Số nhà
                                    63 Nguyễn Huy Tự, Phường Ngọc Trạo, Thành Phố Thanh Hóa, Tỉnh Thanh Hóa, Việt
                                    Nam.
                                </p>
                                <p>Mã số thuế: Số ĐKKD: 2803062864</p>
                                <p>Cấp ngày 06/03/2023 tại phòng TC-KH thành phố Thanh Hóa</p>
                                <p><i class="fa fa-mobile" aria-hidden="true"></i> SĐT: 093 628 27 89</p>
                                <p>Website : <a href="https://bibala.vn">https://bibala.vn</p></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6">
                        <div class="footer_widget">
                            <h3>LIÊN KẾT</h3>
                            <ul>
                                <li><a href="#">Tìm kiếm</a></li>
                                <li><a href="#">Giới thiệu</a></li>
                                <li><a href="#">Chính sách bảo mật</a></li>
                                <li><a href="#">Chính sách và các quy định</a></li>
                                <li><a href="#">Liên hệ</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6">
                        <div class="footer_widget">
                            <h3>Dịch vụ khách hàng</h3>
                            <ul>
                                <li><a href="#">Sản phẩm khuyến mãi</a></li>
                                <li><a href="#">Sản phẩm nổi bật</a></li>
                                <li><a href="#">Tất cả sản phẩm</a></li>
                                <li>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="footer_social text-right">
                                            <ul>
                                                <li><a><i class="fas fa-facebook"></i></a></li>
                                                <li><a href="#"><i class="fas fa-twitter"></i></a></li>
                                                <li>
                                                    <a href="#">
                                                        <i class="fas fa-google-plus"
                                                           aria-hidden="true"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="pinterest" href="#">
                                                        <i class="fas fa-pinterest-p"
                                                           aria-hidden="true"></i>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#"><i class="fas fa-wifi" aria-hidden="true"></i></a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--footer area end-->
    <script src="~/js\vendor\jquery-1.12.0.min.js"></script>
    <script src="~/js\popper.js"></script>
    <script src="~/js\bootstrap.min.js"></script>
    <script src="~/js\ajax-mail.js"></script>
    <script src="~/js\plugins.js"></script>
    <script src="~/js\main.js"></script>
    <script src="~/js/js1.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
